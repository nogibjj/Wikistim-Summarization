<script>
    ; (function ($) {
        $.fn.responsiveMenu = function (options) {
            var settings = $.extend({
                breakWidth: 600,
                label: 'Visit our other sites',
                position: 'left'
            }, options);

            var menu = $(this);

            var clone = menu.clone().insertBefore(menu).removeAttr('class').removeAttr('id');
            var wrapper = $('<div class="responsive-menu rm-' + settings.position + '"></div>');
            var button = $('<button onclick="return false;" class="rm-toggle">' + (settings.label ? '<span class="caret"></span><span class="rm-label">' + settings.label + '</span>' : '') + '</button>');

            wrapper.insertBefore(menu);
            wrapper.prepend(clone);
            wrapper.prepend(button);

            var switchMenu = function () {

                if ($(window).width() <= settings.breakWidth) {
                    wrapper.show();
                    menu.hide();
                } else {
                    wrapper.hide();
                    menu.hide();
                }
            };
            switchMenu();
            button.click(function () {
                clone.toggle();
            });

            $(window).resize(switchMenu);
        };
    })(window.Zepto || window.jQuery);

</script>
