<!-- XML Scroll View for Jobs script start -->
<script type="text/javascript">
//<![CDATA[
var ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget = new aiXmlScrollView();
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.setOutputElement('ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_frame')
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.setItemRoot('Job')
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.setScrollDirection('up');
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.declareVariable('JobTitle', '%jobTitle%');
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.declareVariable('Url', '%jobLink%');
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.declareVariable('Company', '%jobEmployer%');
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.declareVariable('FormattedCityState', '%jobFormattedCityState%');
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.setTemplate('<div class="ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_smWidgetHolder" onclick="window.open(\'%jobLink%\')" onmouseover="this.style.background=\'#F1F1F1\';ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.stopScroll();" onmouseout="this.style.background=\'\';ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.startScroll();"><h3><a hr' + 'ef=\"%jobLink%" onclick="return false" target="_blank">%jobTitle%<\/a><\/h3><ul><li class="ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_smWidgetHolderItem ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_employer">%jobEmployer%<\/li><li class="ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_smWidgetHolderItem ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_location">%jobFormattedCityState%<\/li><\/ul><\/div>');
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.setScrollDelay(45);
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.setScrollIntermittent(4);
ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_Widget.outputFromString(ai_medicaljobs_careers_xmlScrollView_jobs_CNS_Neurosurgery_xmlResults);

//]]>
</script><!-- XML Scroll View for Jobs script end -->
