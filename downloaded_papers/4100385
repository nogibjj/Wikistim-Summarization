<script>
   
    $(function() {
        try {
            var objSearchBox = {
                btnGlobalSearchMagnifierId: 'ctl00_ctl50_Header_SearchTopBoxControl_btnGlobalSearchMagnifier',
                searchButtonId: 'ctl00_ctl50_Header_SearchTopBoxControl_btnGlobalSearch',
                txtKeywordsId: 'ctl00_ctl50_Header_SearchTopBoxControl_txtKeywords',
                hfldKeywordsHiddenId: 'ctl00_ctl50_Header_SearchTopBoxControl_hfldKeywords',
                defaultSearchText: 'Search'
            };

            new Searchbox(objSearchBox).Init();
        } catch (e) {
            console.error(e);
        }
    });

    //autocomplete functionality for basic search
    //this functionality will call the SBA url and start suggesting after typing 3rd character
    var textKeywords = null;
    var hiddenAutocompleteApiUrl = null;
    var buttonGlobalSearch = null;

    $(function () {
        try {
            var objAutocomplete = {
                hiddenIsAutoCompleteEnabled: $('#ctl00_ctl50_Header_SearchTopBoxControl_IsAutoCompleteEnabled'),
                textKeywords: $('#ctl00_ctl50_Header_SearchTopBoxControl_txtKeywords'),
                hiddenAutocompleteApiUrl: $('#ctl00_ctl50_Header_SearchTopBoxControl_AutocompleteApiUrl'),
                buttonGlobalSearch: $('#ctl00_ctl50_Header_SearchTopBoxControl_btnGlobalSearch')
            };

            autoComplete(objAutocomplete);
        } catch (e) {
            console.error(e);
        }
    });

    $(function () {
        let searchSelect = $('select[id$="ddlContentScope"]');;

        if (searchSelect.length){
            searchSelect.selectize();
        }
    });
</script>

