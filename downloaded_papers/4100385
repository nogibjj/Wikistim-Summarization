<script>    
    $(function () {
        function trunc(text, lng) { if (text && text.length > lng) { text = text.substring(0, lng - 4).concat('...'); } return text; }

        $('.wp-feature-carousel p[data-text]').each(function() {
            var $e = $(this);var layout = $('<textarea/>').html($e.attr('data-text')).text();$e.html(layout);
        });

        $('.wp-feature-carousel .carousel-figures').slick({
            arrows: false,
            dots: true,            
            appendDots: '.wp-feature-carousel .carousel-paging',
            customPaging: function () {
                return $('<a href="javascript:void(0);" class="slider-pager-bullet" role="button"><i class="fa fa-circle-o"></i></a>');
            },
            autoplay: true,
            autoplaySpeed: 3000,
            mobileFirst: true,
            responsive: [{
                breakpoint: 991,
                settings: {
                    arrows: true,
                    appendDots: '.wp-feature-carousel .carousel-list',
                    speed: 0,
                    ease: 'linear',
                    swipe: true,
                    waitForAnimate: false,
                    customPaging: function (slider, i) {
                        var slide = slider.$slides[i];
                        var image = slide.querySelector('.js-image');
                        var title = slide.querySelector('.js-title');
                        var layout = '<a href="javascript:void(0);" role="button"><figure class="wp-feature-carousel__list-item\"><img class="list-pic" src="' + image.src +
                            '" alt="' + image.alt + '"><figcaption>' + $(title).html() + '</figcaption></figure></a>';
                        return $(layout);
                    },
                    prevArrow: '<a href="#" data-role="none" class="slick-prev slick-arrow" aria-label="Prev" role="button"></a>',
                    nextArrow: '<a href="#" data-role="none" class="slick-next slick-arrow" aria-label="Next" role="button"></a>'
                }
            }]
        }).on('init afterChange', function (event, slick, direction) {
            objectFitImages();
        });

        $('.wp-feature-carousel .slick-prev').hover(function() {
            $(this).closest('.wp-feature-carousel').addClass('js-highlight-prev');
        }, function () {
            $(this).closest('.wp-feature-carousel').removeClass('js-highlight-prev');
        });

        $('.wp-feature-carousel .slick-next').hover(function () {
            $(this).closest('.wp-feature-carousel').addClass('js-highlight-next');
        }, function () {
            $(this).closest('.wp-feature-carousel').removeClass('js-highlight-next');
        });

        objectFitImages();
    });
</script>








