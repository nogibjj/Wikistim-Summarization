    $(function() {
        try {
            var objSearchBox = {
                btnGlobalSearchMagnifierId: 'ctl00_ctl50_SearchBox_btnGlobalSearchMagnifier',
                searchButtonId: 'ctl00_ctl50_SearchBox_btnGlobalSearch',
                txtKeywordsId: 'ctl00_ctl50_SearchBox_txtKeywords',
                hfldKeywordsHiddenId: 'ctl00_ctl50_SearchBox_hfldKeywords',
                defaultSearchText: 'Search'
            };

            new Searchbox(objSearchBox).Init();
        } catch (e) {
            console.error(e);
        }
    });

    //autocomplete functionality for basic search
    //this functionality will call the SBA url and start suggesting after typing 3rd character
    var textKeywords = null;
    var hiddenAutocompleteApiUrl = null;
    var buttonGlobalSearch = null;

    $(function () {
        try {
            var objAutocomplete = {
                hiddenIsAutoCompleteEnabled: $('#ctl00_ctl50_SearchBox_IsAutoCompleteEnabled'),
                textKeywords: $('#ctl00_ctl50_SearchBox_txtKeywords'),
                hiddenAutocompleteApiUrl: $('#ctl00_ctl50_SearchBox_AutocompleteApiUrl'),
                buttonGlobalSearch: $('#ctl00_ctl50_SearchBox_btnGlobalSearch')
            };

            autoComplete(objAutocomplete);
        } catch (e) {
            console.error(e);
        }
    });

    $(function () {
        let searchSelect = $('select[id$="ddlContentScope"]');;

        if (searchSelect.length){
            searchSelect.selectize();
        }
    });
