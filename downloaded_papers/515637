       class="hfInterstitial"
       data-interstitiallinks="eurheartj/issue,eurheartj/advance-articles,eurheartj/advance-article,eurheartj/supplements,eurheartj/article,eurheartj/article-abstract,eurheartj/pages"
       data-subdomain="eurheartj"/>
    
    
        <script type="text/javascript">
                googletag.cmd.push(function () {
                    
                    googletag.pubads().setTargeting("jnlsdoi", "10.1093/eurheartj/ehi827");
                    googletag.enableServices();
                });
        </script>




    


